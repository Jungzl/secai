  "author": "Jungzl",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "secai": "bin/secai.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jungzl/secai.git"
  },
  "bugs": "https://github.com/jungzl/secai.git",
  "homepage": "https://github.com/jungzl/secai#readme",
  "files": [
    "dist",
    "bin",
    "colors.json"
  ],
  "keywords": [
    "color",
    "traditional",
    "chinese",
    "iroiro",
    "secai"
  ],
  "scripts": {
    "dev": "esno src/cli.ts",
    "build": "esno scripts/build.ts && tsup src/cli.ts src/index.ts --format cjs,esm --dts --minify",
    "prepare": "npm run build",
    "release": "npx bumpp --commit --tag --push && npm publish",
    "web": "npm -C web run dev"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@types/chroma-js": "^2.1.3",
    "@types/color-namer": "^1.3.0",
    "@types/node": "^17.0.21",
    "chroma-js": "^2.4.2",
    "color-namer": "^1.4.0",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2"
  }
}
